{
	"extends": "solium:all",
	"plugins": [ "security" ],
	"rules":
	{
		"blank-lines":               [ "error" ],
		"function-order":            0,
		"indentation":               [ "error", "tab" ],
		"lbrace":                    0,
		"quotes":                    [ "error", "double" ],
		"no-empty-blocks":           0,
		"no-experimental":           0,
		"security/no-block-members": 0
	}
}
